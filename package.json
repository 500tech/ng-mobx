{
  "name": "mobx-angularjs",
  "version": "1.6.1",
  "description": "AngularJS connector to MobX",
  "main": "mobx-angularjs.js",
  "typings": "index.d.ts",
  "author": "Adam Klein",
  "contributors": [
    "Nick Breaton",
    "Kuitos"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobxjs/mobx-angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-angularjs/issues"
  },
  "keywords": [
    "angular",
    "angularjs",
    "mobx-angularjs",
    "mobx",
    "ng-mobx"
  ],
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "webpack --env.type=umd --mode=development && webpack -p --env.type=umd.min --mode=production",
    "example": "parcel example/index.html -o --no-cache --no-hmr -d node_modules/.example",
    "pub": "jest && npm run build && cp {README.md,package.json} dist && npm publish dist",
    "test": "jest && codecov"
  },
  "peerDependencies": {
    "angular": ">=1.4.0",
    "mobx": ">=5.15.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@types/angular": "^1.6.40",
    "@types/angular-mocks": "^1.5.11",
    "@types/jest": "^25.2.1",
    "@types/node": "^9.3.0",
    "angular": "^1.6.8",
    "angular-mocks": "^1.6.8",
    "codecov": "^3.0.0",
    "jest": "^25.2.1",
    "mobx": ">=5.15.4",
    "number-to-words": "^1.2.3",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.3",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "^.+\\.spec\\.(js|ts)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
