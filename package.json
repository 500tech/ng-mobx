{
  "name": "mobx-angularjs",
  "version": "1.5.1",
  "description": "AngularJS connector to MobX",
  "main": "mobx-angularjs.js",
  "typings": "index.d.ts",
  "author": "Adam Klein",
  "contributors": [
    "Nick Breaton",
    "Kuitos"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobxjs/mobx-angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-angularjs/issues"
  },
  "keywords": [
    "angular",
    "angularjs",
    "mobx-angularjs",
    "mobx",
    "ng-mobx"
  ],
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "webpack --env.type=umd && webpack -p --env.type=umd.min",
    "example": "parcel example/index.html -o --no-cache --no-hmr -d node_modules/.example",
    "pub": "jest && npm run build && cp {README.md,package.json} dist && npm publish dist",
    "test": "jest && codecov"
  },
  "peerDependencies": {
    "angular": ">=1.4.0",
    "mobx": ">=2.2.0"
  },
  "devDependencies": {
    "@types/angular": "^1.6.40",
    "@types/angular-mocks": "^1.5.11",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "angular": "^1.6.8",
    "angular-mocks": "^1.6.8",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "jest": "^22.1.2",
    "mobx": "^3.4.1",
    "number-to-words": "^1.2.3",
    "parcel-bundler": "^1.4.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.3.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "^.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
