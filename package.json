{
  "name": "mobx-angularjs",
  "version": "2.0.0",
  "description": "AngularJS connector to MobX",
  "main": "dist/mobx-angularjs.js",
  "typings": "dist/mobx-angularjs.d.js",
  "author": "Adam Klein",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobxjs/mobx-angularjs.git"
  },
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-angularjs/issues"
  },
